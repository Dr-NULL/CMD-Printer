{
  "name": "cmd-printer",
  "version": "1.2.0",
  "description": "Module for print PDF using node.js and SumatraPDF",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "test:cjs": "npm run clean && npm run build:cjs && mocha './dist/cjs/**/*.test.js'",
    "test:esm": "npm run clean && npm run build:esm && mocha './dist/esm/**/*.test.js'",
    "prepare": "npm run clean && npm run build",
    "clean": "./clean.sh",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dr-NULL/CMD-Printer.git"
  },
  "keywords": [
    "printer",
    "windows",
    "cmd"
  ],
  "author": "dr-null <silvaaguilarfelipe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dr-NULL/CMD-Printer/issues"
  },
  "homepage": "https://github.com/Dr-NULL/CMD-Printer#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "codepage": "^1.15.0",
    "uuid": "^8.3.2"
  }
}
